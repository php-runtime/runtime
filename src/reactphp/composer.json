{
    "name": "runtime/react",
    "description": "ReactPHP runtime",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.5",
        "psr/http-server-handler": "^1.0",
        "react/http": "^1.6",
        "symfony/runtime": "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runtime\\React\\": "src/",
            "Symfony\\Runtime\\Psr\\Http\\": "runtime/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runtime\\React\\Tests\\": "tests/"
        }
    }
}
