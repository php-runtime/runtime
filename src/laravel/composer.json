{
    "name": "runtime/laravel",
    "description": "Laravel runtime",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "^8.33",
        "symfony/runtime": "^5.3 || ^6.0"
    },
    "require-dev": {
        "illuminate/http": "^8.33",
        "symfony/console": "^4.4 || ^5.2 || ^6.0",
        "symfony/phpunit-bridge": "^5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runtime\\Laravel\\": "src/",
            "Symfony\\Runtime\\Illuminate\\Contracts\\": "runtime"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runtime\\Laravel\\Tests\\": "tests/"
        }
    }
}
