{
    "name": "runtime/runtime",
    "type": "library",
    "description": "Includes all runtimes",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "bref/bref": "^1.2",
        "clue/arguments": "^2.1",
        "illuminate/contracts": "^8.33",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "react/http": "^1.2",
        "spiral/roadrunner": "^2.0",
        "symfony/console": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/runtime": "5.x-dev"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "illuminate/http": "^8.33",
        "swoole/ide-helper": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Runtime\\Bref\\": "src/bref/src",
            "Runtime\\Laravel\\": "src/laravel/src",
            "Runtime\\PsrNyholmLaminas\\": "src/psr-nyholm-laminas/src",
            "Runtime\\React\\": "src/react/src",
            "Runtime\\RoadRunnerNyholm\\": "src/roadrunner-nyholm/src",
            "Runtime\\RoadRunnerSymfonyNyholm\\": "src/roadrunner-symfony-nyholm/src",
            "Runtime\\Swoole\\": "src/swoole/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
